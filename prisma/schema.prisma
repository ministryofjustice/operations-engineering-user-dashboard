generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id
  name String
  email String
  github_username String
  slack_username String
  docker Boolean
}

model DockerUser {
  id Int @id
  username String
  email String
}
